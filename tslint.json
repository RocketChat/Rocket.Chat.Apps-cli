{
  "extends": "tslint:recommended",
  "rules": {
    "array-type": [true, "generic"],
    "member-access": true,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "promise-function-async": true,
    "variable-name": false
  }
}
